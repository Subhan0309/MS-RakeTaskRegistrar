{
  "name": "MS-RakeTaskRegistrar",
  "displayName": "RakeTaskRegistrar",
  "description": "VS Code extension for registering rake tasks in migrations with CodeLens support",
  "version": "0.0.1",
  "publisher": "MuhammadSubhan",
  "repository": {
    "type": "git",
    "url": "https://github.com/Subhan0309/MS-RakeTaskRegistrar.git"
  },
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:ruby"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "MS-RakeTaskRegistrar.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "rakeTaskMigration.registerTask",
        "title": "Register Rake Task in Migration"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test",
    "vscode:prepublish": "npm run lint"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.101.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1"
  },
  "dependencies": {
    "vsce": "^2.15.0"
  }
}
