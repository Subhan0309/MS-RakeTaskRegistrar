{
  "name": "MS-RakeTaskRegistrar",
  "displayName": "RakeTaskRegistrar",
  "description": "VS Code extension for registering rake tasks in migrations with CodeLens support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:ruby"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "MS-RakeTaskRegistrar.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "rakeTaskMigration.registerTask",
        "title": "Register Rake Task in Migration"
      }
    ]
  },
  "files": [
    "extension.js",
    "scripts/**/*",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test",
    "vscode:prepublish": "npm run lint"
  },
  "devDependencies": {
    "@types/vscode": "^1.101.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.25.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}